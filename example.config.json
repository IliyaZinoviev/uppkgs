{
  "source": "/path_to_repos/",
  "gitlab_host": "https://gitlab.example.com/api/v4/",
  "repo_keys": [
    "some_repo_dir_name"
  ],
  "ticket": "ticket",
  "commit_message": "commit_message",
  "repos": {
    "some_repo_dir_name": {
      "packages": [
        "some_package"
      ],
      "target_branch": "master",
      "project_id": 1,
      "mr_id": 1,
      "is_need_changes": true,
      "branch": {
        "dir": "feature",
        "suffix": "some_feature"
      },
      "pre_handler": "some_command",
      "post_handler": "some_command"
    }
  },
  "packages": {
    "some_package": "some_package@latest"
  },
  "assignee_id": 1,
  "reviewer_ids": [
    1,
    2
  ],
  "token": "some_token"
}
